      <eParameters name="child" lowerBound="1" eType="#//ConceptSetDefinition"/>
    </eOperations>
    <eOperations name="removeChild" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
