<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
	<profiles>
		<profile>
			<id>repositories</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<artifactory.url>${ARTIFACTORY_URL}</artifactory.url>
				<artifactory.snapshots.url>${artifactory.url}/babylon-maven-snapshots</artifactory.snapshots.url>
				<artifactory.releases.url>${artifactory.url}/babylon-maven-releases</artifactory.releases.url>
			</properties>
		</profile>
	</profiles>
	<servers>
		<server>
			<id>babylon-snapshots</id>
			<username>${env.ARTIFACTORY_USER}</username>
			<password>${env.ARTIFACTORY_PWD}</password>
		</server>
		<server>
			<id>babylon-releases</id>
			<username>${env.ARTIFACTORY_USER}</username>
			<password>${env.ARTIFACTORY_PWD}</password>
		</server>
	</servers>

</settings>