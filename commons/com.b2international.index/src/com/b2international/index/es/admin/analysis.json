{
	"analyzer": {
		"tokenized": {
			"tokenizer": "whitespace",
			"filter": [
				"asciifolding",
				"lowercase",
				"possessive",
				"word_splitter",
				"unique_token"
			]
		},
		"exact": {
			"tokenizer": "keyword",
			"filter": [
				"asciifolding",
				"lowercase"
			]
		},
		"search_stemming": {
			"tokenizer": "whitespace",
			"filter": [
				"asciifolding",
				"lowercase",
				"possessive",
				"unique_token",
				"english_stemmer"
			]
		},
		"stemming": {
			"tokenizer": "whitespace",
			"filter": [
				"asciifolding",
				"lowercase",
				"possessive",
				"word_splitter",
				"unique_token",
				"english_stemmer"
			]
		},
		"case_sensitive_ascii": {
			"tokenizer": "whitespace",
			"filter": [
				"asciifolding",
				"possessive",
				"word_splitter",
				"unique_token"
			]
		},
		"case_sensitive_ascii_exact": {
			"tokenizer": "keyword",
			"filter": [
				"asciifolding"
			]
		},
		"case_sensitive": {
			"tokenizer": "whitespace",
			"filter": [
				"possessive",
				"word_splitter",
				"unique_token"
			]
		},
		"prefix": {
			"tokenizer": "whitespace",
			"filter": [
				"asciifolding",
				"lowercase",
				"possessive",
				"word_splitter",
				"unique_token",
				"edge_ngram"
			]
		},
		"case_sensitive_prefix": {
			"tokenizer": "whitespace",
			"filter": [
				"asciifolding",
				"possessive",
				"word_splitter",
				"unique_token",
				"edge_ngram"
			]
		}
	},
	"normalizer": {
		"lowerascii": {
			"type": "custom",
			"char_filter": [],
			"filter": [
				"lowercase",
				"asciifolding"
			]
		}
	},
	"filter": {
		"english_stemmer": {
			"type": "stemmer",
			"name": "english"
		},
		"edge_ngram": {
			"type": "edge_ngram",
			"min_gram": 1,
			"max_gram": 10
		},
		"possessive": {
			"type": "stemmer",
			"name": "possessive_english"
		},
		"word_splitter": {
			"type": "word_delimiter",
			"split_on_case_change": "false",
			"split_on_numerics": "false",
			"preserve_original": "true",
			"stem_english_possessive": "false",
			"type_table": [
				", => DIGIT",
				". => DIGIT"
			]
		},
		"unique_token": {
			"type": "unique",
			"only_on_same_position": "true"
		}
	}
}